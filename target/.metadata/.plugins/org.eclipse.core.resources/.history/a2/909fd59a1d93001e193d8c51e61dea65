

#include <stdio.h>

int main(){
	float number = 12.12345678910;

	printf("%d", number); //int is not big enough
	printf("%f", number); //float loses precision
	printf("%0.2f", number); //2 places after decimal
	printf("%0.2e", number); //Scientific Notation

	float chargeOfElectron = -1.60217662e-19;

	return 0;
}
