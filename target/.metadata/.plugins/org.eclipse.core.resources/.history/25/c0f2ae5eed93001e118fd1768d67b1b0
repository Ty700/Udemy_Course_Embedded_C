//Turning on the PD12 LED

#include <stdint.h>

#define CCREG_ADDR (uint32_t *)0x40023830
#define GPIOD_MODE_ADDR (uint32_t *)0x40020C00
#define GPIOD_OUTPUT_ADDR (uint32_t *)0x50020C14

int main(void)
{
    //Clock for GPIOD reg addr
	uint8_t *p_ClkCtrlReg = CCREG_ADDR;

	//Setting 4th bit of CCREG_ADDR - controls clock for GPIOD
	*p_ccreg_addr |= 16;

	for(;;);
}
