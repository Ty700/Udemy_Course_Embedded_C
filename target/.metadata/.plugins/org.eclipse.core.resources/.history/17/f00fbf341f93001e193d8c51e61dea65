#include <stdio.h>

int main(void){
	float num1, num2, num3;

	printf("Enter number 1: ");
		scanf("%f", &num1);
	printf("Enter number 2: ");
		scanf("%f", &num2);
	printf("Enter number 3: ");
		scanf("%f", &num3);

	printf("Total: %f", (float)(num1+num2+num3)/3);

	return 0;
}
